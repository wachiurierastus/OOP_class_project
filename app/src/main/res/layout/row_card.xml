<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cardRowModel" type="com.taskmanagement.app.modules.employeetaskmanagementdashboard.data.model.CardRowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameCardmenu1"
        android:layout_height="wrap_content" android:layout_width="@dimen/_129pxh">
        <FrameLayout android:id="@+id/frameCardmenu2"
            android:layout_gravity="start"
            android:layout_height="@dimen/_188pxv"
            android:layout_width="@dimen/_129pxh" style="@style/groupStyleblue_700cornerRadius">
            <ImageView android:id="@+id/imageCardmenubackg"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_258pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_261pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_cardmenubackg}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_cardmenubackg"/>
        </FrameLayout>
        <LinearLayout android:id="@+id/linearColumnduration"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_13pxv"
            android:layout_marginEnd="@dimen/_9pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearCardmenudeadl"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtDuration"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{cardRowModel.txtDuration}"
                    style="@style/txtPoppinsregular10" tools:text="@string/lbl_10_days"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearCardmenutitle"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_41pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCardmenutitle"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_cardmenutitle"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_cardmenutitle"/>
                <TextView android:id="@+id/txtParaprimaryce"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_weight="0.81"
                    android:layout_width="0dp"
                    android:text="@{cardRowModel.txtParaprimaryce}"
                    style="@style/txtPoppinssemibold16" tools:text="@string/lbl_make_breakfast"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearCardmenuprogr"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtParaprimaryceOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{cardRowModel.txtParaprimaryceOne}"
                    style="@style/txtPoppinsregular10_1" tools:text="@string/lbl_progress"/>
                <FrameLayout android:id="@+id/frameStackcardmenuprogr"
                    android:layout_height="@dimen/_18pxv"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_109pxh">
                    <ProgressBar
                        android:id="@+id/progressBarCardmenuprogr"
                        android:indeterminate="false"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_width="match_parent"
                        android:max="100"
                        android:minHeight="@dimen/_4pxv"
                        android:progress="79"
                        android:progressDrawable="@drawable/layer_list_radius_2" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtParaprimaryceTwo"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{cardRowModel.txtParaprimaryceTwo}"
                        style="@style/txtPoppinsregular10_1" tools:text="@string/lbl_80"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
