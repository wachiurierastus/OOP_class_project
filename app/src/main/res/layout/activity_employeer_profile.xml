<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="employeerProfileVM" type="com.taskmanagement.app.modules.employeerprofile.data.viewmodel.EmployeerProfileVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearEmployeerProfi"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnsubtract"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStacklanguage"
                android:layout_height="@dimen/_264pxv" android:layout_width="match_parent">
                <FrameLayout android:id="@+id/frameStacksubtract"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_177pxv"
                    android:layout_marginBottom="@dimen/_10pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageSubtract"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_177pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_subtract"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_subtract"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtParaprimaryce"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{employeerProfileVM.employeerProfileModel.txtParaprimaryce}"
                        style="@style/txtPoppinssemibold16" tools:text="@string/lbl_profile"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameProfile"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_220pxv"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_345pxh">
                    <LinearLayout android:id="@+id/linearColumnlanguage"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_3">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtLanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_70pxh"
                            android:layout_marginStart="@dimen/_70pxh"
                            android:layout_marginTop="@dimen/_65pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{employeerProfileVM.employeerProfileModel.txtLanguage}"
                            style="@style/txtPoppinssemibold16_1" tools:text="@string/msg_nyakimincha_nya"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowline216"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_27pxv"
                            android:layout_marginEnd="@dimen/_85pxh"
                            android:layout_marginStart="@dimen/_85pxh"
                            android:layout_marginTop="@dimen/_32pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <View
                                android:background="@color/blue_700_3a"
                                android:id="@+id/lineLine216"
                                android:layout_height="@dimen/_25pxv"
                                android:layout_weight="0.02" android:layout_width="0dp"/>
                            <ImageView android:id="@+id/imageVector"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="@dimen/_16pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector_blue_700_20x16"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_700_20x16"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtLanguageOne"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_marginStart="@dimen/_6pxh"
                                android:layout_marginTop="@dimen/_6pxv"
                                android:layout_weight="0.79"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{employeerProfileVM.employeerProfileModel.txtLanguageOne}"
                                style="@style/txtPoppinsregular10_2" tools:text="@string/lbl_26_employees"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView android:id="@+id/imagePhotoOne"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_102pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_104pxh"
                        android:layout_marginStart="@dimen/_104pxh"
                        android:layout_width="@dimen/_123pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_photo153172799}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_photo153172799"/>
                    <ImageView android:id="@+id/imagePhotoTwo"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_107pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_104pxh"
                        android:layout_marginStart="@dimen/_104pxh"
                        android:layout_width="@dimen/_123pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_photo153172799_107x123}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_photo153172799_107x123"/>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearButtonprimary"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_49pxh"
                android:layout_marginStart="@dimen/_49pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVectorOne"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_blue_700_20x16"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_700_20x16"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMyProfile"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_34pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{employeerProfileVM.employeerProfileModel.txtMyProfile}"
                    style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_my_profile"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearButtonprimary1"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_47pxh"
                android:layout_marginStart="@dimen/_47pxh"
                android:layout_marginTop="@dimen/_44pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVectorTwo"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_blue_700_20x19"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_700_20x19"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSettings"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{employeerProfileVM.employeerProfileModel.txtSettings}"
                    style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_settings"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearButtonprimary2"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_46pxh"
                android:layout_marginStart="@dimen/_46pxh"
                android:layout_marginTop="@dimen/_48pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVectorThree"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_blue_700_20x20"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_700_20x20"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLogout"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{employeerProfileVM.employeerProfileModel.txtLogout}"
                    style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_logout"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_width="match_parent" style="@style/groupStylewhite_5">
            <LinearLayout android:id="@+id/linearRowvectorFour"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_26pxv"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVectorFour"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_blue_100_26x25"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_100_26x25"/>
                <ImageView android:id="@+id/imageVectorFive"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_96pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_blue_100"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_100"/>
                <ImageView android:id="@+id/imageVectorSix"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginEnd="@dimen/_55pxh"
                    android:layout_marginStart="@dimen/_98pxh"
                    android:layout_width="@dimen/_21pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_blue_700_26x21"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_700_26x21"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
