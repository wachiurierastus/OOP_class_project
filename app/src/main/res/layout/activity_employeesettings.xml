<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="employeesettingsVM" type="com.taskmanagement.app.modules.employeesettings.data.viewmodel.EmployeesettingsVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_22pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblue_700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_white_a700_radius_10"
                    android:id="@+id/btnArrowleft"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_width="@dimen/_36pxh"
                    android:paddingHorizontal="@dimen/_10pxh"
                    android:paddingVertical="@dimen/_10pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtParaprimaryce"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_98pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_weight="0.65"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{employeesettingsVM.employeesettingsModel.txtParaprimaryce}"
                    style="@style/txtPoppinssemibold16" tools:text="@string/lbl_settings"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_42pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleblue_700">
            <LinearLayout android:id="@+id/linearColumnvector"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_4">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearButtonprimary"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_61pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylegray_100">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_48pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_blue_700_21x18"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_700_21x18"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtNotification"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_206pxh"
                        android:layout_marginStart="@dimen/_33pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.79"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{employeesettingsVM.employeesettingsModel.txtNotification}"
                        style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_notification"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearButtonprimary1"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_46pxh"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_44pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVectorOne"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_width="@dimen/_21pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_blue_700_21x21"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_700_21x21"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHelp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_31pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.56"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{employeesettingsVM.employeesettingsModel.txtHelp}"
                        style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_help"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearButtonprimary2"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_47pxh"
                    android:layout_marginStart="@dimen/_47pxh"
                    android:layout_marginTop="@dimen/_66pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVectorTwo"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_blue_700_18x18"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_700_18x18"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAbout"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_32pxh"
                        android:layout_weight="0.66"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{employeesettingsVM.employeesettingsModel.txtAbout}"
                        style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_about"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearButtonprimary3"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_322pxv"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_66pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imagePeoplesharp"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_peoplesharp"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_peoplesharp"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtInviteafriend"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_30pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.78"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{employeesettingsVM.employeesettingsModel.txtInviteafriend}"
                        style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_invite_a_friend"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
