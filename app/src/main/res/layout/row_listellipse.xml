<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listellipseRowModel" type="com.taskmanagement.app.modules.viewemployeestaskmanagement.data.model.ListellipseRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowellipse"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageEllipse170"
            android:layout_height="@dimen/_45pxh"
            android:layout_width="@dimen/_45pxh"
            android:scaleType="centerCrop" circular="@{true}"
            imageRes="@{@drawable/img_ellipse171}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse171"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearParaprimaryle"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_weight="0.7" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtJacobKamau"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{listellipseRowModel.txtJacobKamau}"
                style="@style/txtPoppinsregular12_3" tools:text="@string/lbl_jacob_kamau"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageOne"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{listellipseRowModel.txtLanguageOne}"
                style="@style/txtPoppinsregular10_7" tools:text="@string/msg_254_812_9875"/>
        </LinearLayout>
    </LinearLayout>
</layout>
