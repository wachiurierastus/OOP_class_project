<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="registrationpageVM" type="com.taskmanagement.app.modules.registrationpage.data.viewmodel.RegistrationpageVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearRegisterTask"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <TextView android:ellipsize="end"
            android:id="@+id/txtTASKLinker"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_68pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{registrationpageVM.registrationpageModel.txtTASKLinker}"
            style="@style/txtRighteousregular30" tools:text="@string/lbl_task_linker"/>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtLanguage"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{registrationpageVM.registrationpageModel.txtLanguage}"
            style="@style/txtPoppinsmedium16" tools:text="@string/msg_task_management"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtCreateyouracc"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_48pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{registrationpageVM.registrationpageModel.txtCreateyouracc}"
            style="@style/txtPoppinsmedium14_2" tools:text="@string/msg_create_your_acc"/>
        <FrameLayout android:id="@+id/frameInputprimaryd"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_48pxv"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_27pxv" android:layout_width="@dimen/_345pxh">
            <ImageButton
                android:background="@drawable/rectangle_bg_blue_700_radius_tl_10_bl_10"
                android:id="@+id/btnProfile"
                android:layout_gravity="start"
                android:layout_height="@dimen/_48pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="@dimen/_50pxh"
                android:paddingHorizontal="@dimen/_17pxh"
                android:paddingVertical="@dimen/_17pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_profile" tools:ignore="ContentDescription"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnUsername"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true"
                android:text="@string/lbl_username"
                android:textAllCaps="false"
                style="@style/btnRoundedOutline" tools:text="@string/lbl_username"/>
        </FrameLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowvector"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius_2">
            <FrameLayout android:id="@+id/frameMessage"
                android:layout_height="@dimen/_48pxv"
                android:layout_width="@dimen/_50pxh" style="@style/groupStyleblue_1">
                <ImageView android:id="@+id/imageVector"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_white_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700"/>
            </FrameLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtEmail"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_241pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="0.4" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{registrationpageVM.registrationpageModel.txtEmail}"
                style="@style/txtPoppinsregular12_2" tools:text="@string/lbl_email"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowvectorOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius_2">
            <FrameLayout android:id="@+id/frameLock"
                android:layout_height="@dimen/_47pxv"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_50pxh" style="@style/groupStyleblue_1">
                <ImageView android:id="@+id/imageVectorOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPassword"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_216pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="0.54" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{registrationpageVM.registrationpageModel.txtPassword}"
                style="@style/txtPoppinsregular12_2" tools:text="@string/lbl_password"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowvectorTwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius_2">
            <FrameLayout android:id="@+id/frameLock1"
                android:layout_height="@dimen/_47pxv"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_50pxh" style="@style/groupStyleblue_1">
                <ImageView android:id="@+id/imageVectorTwo"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtConfirmPasswor"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_164pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="0.69" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{registrationpageVM.registrationpageModel.txtConfirmPasswor}"
                style="@style/txtPoppinsregular12_2" tools:text="@string/msg_confirm_passwor"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtLanguageOne"
            android:layout_gravity="end"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{registrationpageVM.registrationpageModel.txtLanguageOne}"
            style="@style/txtPoppinsregular10_3" tools:text="@string/msg_already_registe"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnRegister"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_14pxv" android:singleLine="true"
            android:text="@string/lbl_register"
            android:textAllCaps="false" style="@style/btnSolidRounded" tools:text="@string/lbl_register"/>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearParaprimaryce"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <View android:background="@color/blue_700"
                android:id="@+id/lineLine137"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_11pxv"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_weight="0.08" android:layout_width="0dp"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrRegisterwit"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_weight="0.84" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{registrationpageVM.registrationpageModel.txtOrRegisterwit}"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_or_register_wit"/>
            <View android:background="@color/blue_700"
                android:id="@+id/lineLine138"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_11pxv"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_weight="0.08" android:layout_width="0dp"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearButtonprimary"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_47pxv"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageButton
                android:background="@drawable/rectangle_border_blue_700_19_radius_10"
                android:id="@+id/btnButtonprimary"
                android:layout_height="@dimen/_71pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_70pxh"
                android:paddingHorizontal="@dimen/_20pxh"
                android:paddingVertical="@dimen/_20pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_buttonprimary_71x70" tools:ignore="ContentDescription"/>
            <ImageButton
                android:background="@drawable/rectangle_border_blue_700_19_radius_10"
                android:id="@+id/btnButtonprimaryOne"
                android:layout_height="@dimen/_71pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_70pxh"
                android:paddingHorizontal="@dimen/_20pxh"
                android:paddingVertical="@dimen/_20pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_buttonprimary" tools:ignore="ContentDescription"/>
            <ImageButton
                android:background="@drawable/rectangle_border_blue_700_19_radius_10"
                android:id="@+id/btnButtonprimaryTwo"
                android:layout_height="@dimen/_71pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_width="@dimen/_70pxh"
                android:paddingHorizontal="@dimen/_20pxh"
                android:paddingVertical="@dimen/_20pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_buttonprimary_light_blue_500" tools:ignore="ContentDescription"/>
        </LinearLayout>
    </LinearLayout>
</layout>
